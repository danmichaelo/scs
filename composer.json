{
    "name": "scriptotek/lsm",
    "description": "University of Oslo Library Services Middleware",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Dan Michael O. Hegg√∏",
            "email": "d.m.heggo@ub.uio.no"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/danmichaelo/primo-services"
        },
        {
            "type": "vcs",
            "url": "https://github.com/danmichaelo/Swagger"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "bclibraries/primo-services": "dev-httplug",
        "danmichaelo/quitesimplexmlelement": "^1.0",
        "digitalbazaar/json-ld": "~0.4",
        "latrell/swagger": "~1.0.2",
        "scriptotek/alma-client": "@dev",
        "scriptotek/marc": "@dev",
        "guzzlehttp/guzzle": "^6.2",
        "php-http/httplug": "^1.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/discovery": "^1.2",
        "php-http/message-factory": "^1.0",
        "php-http/message": "^1.5",
        "sentry/sentry-laravel": "^0.8",
        "laravel/framework": "5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "fzaninotto/faker": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
