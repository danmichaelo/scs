{
    "name": "scriptotek/lsm",
    "description": "University of Oslo Library Services Middleware",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Dan Michael O. HeggÃ¸",
            "email": "d.m.heggo@ub.uio.no"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/danmichaelo/primo-services"
        },
        {
            "type": "vcs",
            "url": "https://github.com/danmichaelo/Swagger"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "~2.2",
        "bclibraries/primo-services" : "dev-httplug",
        "danmichaelo/quitesimplexmlelement": "~0.4",
        "palanik/lumen-cors": "~0.0.2",
        "digitalbazaar/json-ld": "~0.4",
        "laravelista/lumen-vendor-publish": "^2.0",
        "latrell/swagger": "~1.0.2",
        "scriptotek/alma-client": "^0.6.0",
        "guzzlehttp/guzzle": "^6.2",
        "php-http/httplug": "^1.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/discovery": "^1.2",
        "php-http/message-factory": "^1.0",
        "php-http/message": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
